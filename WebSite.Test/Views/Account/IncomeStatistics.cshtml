
@{
    ViewBag.Title = "我的收益统计";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 style="text-align: center;">我的收益统计</h2>
<form class="form-horizontal" action="/Account/IncomeStatistics" method="post">
    <div class="form-group">
        <label for="txtUserId" class="col-sm-1 control-label">userid</label>
        <div class="col-sm-10">
            <input name="userId" type="text" class="form-control" id="txtUserId" placeholder="用户Id">
        </div>
    </div>

    <div class="form-group">
        <label for="txtToken" class="col-sm-1 control-label">token</label>
        <div class="col-sm-10">
            <input name="token" type="text" class="form-control" id="txtToken" placeholder="用户Token">
        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-offset-1 col-sm-10">
            <button type="submit" class="btn btn-primary">提交</button>
        </div>
    </div>

    <div class="form-group">
        <label for="txtResult" class="col-sm-1 control-label">Result</label>
        <div class="col-sm-10">
            <textarea class="form-control" id="txtResult" rows="5">@ViewData["Result"]</textarea>
        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-offset-1 col-sm-10">
            <h3>接口返回参数说明：</h3>
            <p><strong>CurrentIncome：</strong>当前收益额</p>
            <p><strong>TotalIncome：</strong>累计收益额</p>
            <p><strong>TotalInviteUser：</strong>总邀请用户数</p>
            <p><strong>TotalLiveCallTime：</strong>视频累计时长（分钟）</p>
            <p><strong>TotalGiftCount：</strong>礼物累计个数</p>
        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-offset-1 col-sm-10">
            <p><a href="~/Default/Index" class="btn btn-default" style="text-align:left" role="button">返回接口列表</a></p>
        </div>
    </div>
</form>



