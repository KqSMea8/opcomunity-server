
@{
    ViewBag.Title = "加密原始密码";
}
<h2 style="text-align: center;">加密原始密码</h2>
<form class="form-horizontal" action="/Login/EncryptPassword" method="post">
    <div class="form-group">
        <label for="txtPassword" class="col-sm-1 control-label">password</label>
        <div class="col-sm-10">
            <input name="password" type="text" class="form-control" id="txtPassword" placeholder="原始密码">
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-1 col-sm-10">
            <button type="submit" class="btn btn-primary">提交</button>
        </div>
    </div>
    <div class="form-group">
        <label for="txtResult" class="col-sm-1 control-label">Result</label>
        <div class="col-sm-10">
            <textarea class="form-control" id="txtResult" rows="5">@ViewData["Result"]</textarea>
        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-offset-1 col-sm-10">
            <p><a href="~/Default/Index" class="btn btn-default" style="text-align:left" role="button">返回接口列表</a></p>
        </div>
    </div>
</form>

